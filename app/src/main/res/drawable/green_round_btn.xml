<?xml version="1.0" encoding="utf-8"?><!--<vector xmlns:android="http://schemas.android.com/apk/res/android"-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <layer-list>
            <!-- SHADOW -->
            <item>
                <shape>
                    <solid android:color="#02531A" />
                    <corners android:radius="19dp" />
                </shape>
            </item>
            <!-- BUTTON android:right="10px" to make it round-->
            <item android:bottom="15px">
                <shape>
                    <padding android:bottom="5dp" />
                    <gradient android:angle="270" android:endColor="#019138" android:startColor="#00C853" />
                    <corners android:radius="19dp" />
                    <padding android:bottom="15dp" android:left="10dp" android:right="5dp" android:top="10dp" />
                </shape>
            </item>
        </layer-list>
    </item>

    <item android:state_pressed="true">
        <layer-list>
            <!-- SHADOW -->
            <item>
                <shape>
                    <solid android:color="#102746" />
                    <corners android:radius="19dp" />

                </shape>
            </item>
            <!-- BUTTON -->
            <item android:bottom="15px">
                <shape>
                    <padding android:bottom="5dp" />
                    <gradient android:angle="270" android:endColor="#163969" android:startColor="#1c4985" />
                    <corners android:radius="19dp" />
                    <padding android:bottom="15dp" android:left="10dp" android:right="5dp" android:top="10dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
